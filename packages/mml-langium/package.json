{
  "name": "model-modeling-language-langium",
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "build": "tsc -b tsconfig.json && node esbuild.mjs",
    "build:dependencies": "npm run langium:generate && npm run buf:generate",
    "watch": "concurrently -n tsc,esbuild -c blue,yellow \"tsc -b tsconfig.json --watch\" \"node esbuild.mjs --watch\"",
    "lint": "eslint src --ext ts",
    "langium:generate": "langium generate",
    "langium:watch": "langium generate --watch",
    "vscode:prepublish": "npm run build && npm run lint",
    "buf:generate": "cd src/extension/proto && buf generate"
  },
  "dependencies": {
    "@types/uuid": "^9.0.0",
    "@vscode/webview-ui-toolkit": "~1.4.0",
    "@vscode/codicons": "~0.0.35",
    "uuid": "^9.0.1",
    "langium": "~2.1.3",
    "vscode-languageclient": "~9.0.1",
    "vscode-languageserver": "~9.0.1",
    "chalk": "~5.3.0",
    "commander": "~11.0.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "sprotty": "~1.0.0",
    "sprotty-vscode": "~1.0.0"
  },
  "devDependencies": {
    "@types/react": "~18.2.64",
    "@types/react-dom": "~18.2.21",
    "@types/node": "~20.10.4",
    "@types/vscode": "~1.67.0",
    "@typescript-eslint/parser": "~6.14.0",
    "@typescript-eslint/eslint-plugin": "~6.14.0",
    "@bufbuild/protobuf": "~1.7.0",
    "@bufbuild/protoc-gen-es": "~1.7.0",
    "@bufbuild/buf": "~1.29.0",
    "@connectrpc/connect": "~1.3.0",
    "@connectrpc/connect-node": "~1.3.0",
    "@connectrpc/protoc-gen-connect-es": "~1.3.0",
    "eslint": "~8.55.0",
    "langium-cli": "~2.1.0",
    "typescript": "~5.3.3",
    "concurrently": "~8.2.2",
    "esbuild": "~0.19.9",
    "vitest": "~1.4.0"
  },
  "engines": {
    "vscode": "^1.67.0",
    "node": ">=16.0.0"
  }
}
