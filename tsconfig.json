{
  "compilerOptions": {
    "sourceMap": true,
    "target": "ES2017",
    "noUnusedLocals": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "strict": true,
    "jsx": "react",
    "skipLibCheck": true,
  },
  "include": [
    "**/src/**/*"
  ],
  "exclude": [
    "**/out/**/*",
    "**/node_modules/**/*"
  ]
}
